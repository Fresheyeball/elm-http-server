{
    "version": "1.0.0",
    "summary": "Simple bindings to Node.js's Http.Server as a Signal (Request, Response)",
    "repository": "https://github.com/Fresheyeball/elm-http-server.git",
    "license": "MIT",
    "source-directories": [
        "src",
        "example/isomorphic-templating",
        "example/simple"
    ],
    "exposed-modules": [
        "Http.Server"
    ],
    "native-modules": true,
    "dependencies": {
        "elm-lang/core": "2.1.0 <= v < 3.0.0",
        "evancz/elm-html": "4.0.1 <= v < 5.0.0",
        "evancz/elm-http": "2.0.0 <= v < 3.0.0",
        "evancz/virtual-dom": "2.0.0 <= v < 3.0.0"
    },
    "elm-version": "0.15.1 <= v < 0.16.0"
}
